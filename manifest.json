{
  "name": "Incognito Tab extension",
  "description": "1 click shortcut to toggle between incognito.",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "incognito-icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["tabs"],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+I"
      },
      "description": "Runs background.js"
    }
  },
  "icons": {
    "16": "incognito-icon.png",
    "48": "incognito-icon.png",
    "128": "incognito-icon.png"
  }
}
